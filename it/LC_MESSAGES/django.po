#. Page path: /1-1/key-features/
#. Translators:
#. Page path: /1-3T/key-features/
#. Translators:
#. Page path: /1-4/key-features/
#. Translators:
#. Page path: /2-0/key-features/
#: db-strings.txt:681 db-strings.txt.py:3102 db-strings.txt.py:5649
#: db-strings.txt:8061
msgid "Contacts"
msgstr "Contatti"

#. Translators:
#. Page path: /1-1/key-features/
#. Translators:
#. Page path: /1-3T/key-features/
#. Translators:
#. Page path: /1-4/key-features/
#: db-strings.txt:690 db-strings.txt.py:3111 db-strings.txt.py:5658
msgid ""
"<p>Stay connected, with easy access to your friends and colleagues from "
"wherever you are.</p>"
msgstr ""
"<p>Rimani in contatto con i tuoi amici e colleghi ovunque ti trovi.</p>"

